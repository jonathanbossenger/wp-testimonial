{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wp-testimonial/testimonial-block",
	"version": "0.1.0",
	"title": "Testimonial",
	"category": "widgets",
	"icon": "smiley",
	"description": "Add a testimonial to your post or page.",
	"supports": {
		"html": false
	},
  "attributes": {
    "client": {
      "type": "string",
      "default": "Client Name"
    },
    "testimonial": {
      "type": "string",
      "default": "Client Testimonial"
    }
  },
	"textdomain": "wp-testimonial",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
